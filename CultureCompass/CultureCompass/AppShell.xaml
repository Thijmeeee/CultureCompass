<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="CultureCompass.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:CultureCompass"
    xmlns:ui="clr-namespace:CultureCompass.UI"
    xmlns:strings="clr-namespace:CultureCompass.Resources.Strings"
    Shell.FlyoutBehavior="Disabled"
    Title="{x:Static strings:AppResources.AppName}">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate ui:MainPage}"
        Route="MainPage" />

</Shell>
